.Submit {
    width: 150px;
    height: 50px;
    background-color: white;
    border: #019082 solid 1px;
    border-radius: 10px;
    color: #019082;
}

.button-reciting {
    width: 100%;
    text-align: center;
    margin-top: 142px;

}



.contentsManager-show {
    display: block;
    background-color: antiquewhite;
    width: 90%;
    height: 910px;
    float: left;
}
.contentsManager-hide {
    display: none;
}

.selectInput {
    width: 100px;
    height: 30px;
    margin-left: 10px;
    margin-top: 30px;
}
.exploreInput {
    margin-top: 30px;
    height: 25px;
    width: 300px;
}
.exploreBtn {
    width: 60px;
    height: 25px;
    margin-top: 35px;
}

.questionTable td {
    padding: 5px 20px;
}

.reciteManagerContent {
    display: none;
}


