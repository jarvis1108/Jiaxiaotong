/**
 * @Author: meiyu
 * @Date: 2018/7/5 15:01
 */
@RunWith(SpringRunner.class)
@SpringBootTest
public class CouponDetailServiceImplTest {
    @Autowired
    private CouponDetailService couponDetailService;

    @Test
    public void getCouponDetailList() throws ErrorInfo {
        List<CouponDetail> consumeRecordList = couponDetailService.getCouponDetailList(2,1);
        assertNotEquals(1, consumeRecordList.size());
        consumeRecordList.forEach(System.out::println);
    }

    @Test
    public void create() throws ErrorInfo {
        CouponDetail couponDetail = new CouponDetail();
        couponDetail.setBeginTime(Timestamp.valueOf("2018-06-05 12:12:12"));
        couponDetail.setCouponExchangeId("1");
        couponDetail.setCouponExchangeNo("1");
        couponDetail.setCouponId("1");
        couponDetail.setCouponNo("1");
        couponDetail.setCouponStatus("100");
        couponDetail.setEndTime(Timestamp.valueOf("2018-07-05 12:12:12"));
        couponDetail.setOrderRecordId("1");
        couponDetail.setUserId("1");
        couponDetail.setCreateTime(Timestamp.valueOf("2018-07-05 12:12:12"));
        couponDetail.setCreateUser("1");
        couponDetail.setIsValid(Byte.valueOf("1"));
        couponDetail.setLastUpdTime(Timestamp.valueOf("2018-07-06 12:12:12"));
        couponDetail.setLastUpdUser("test");
        couponDetail.setRemark("test");

        CouponDetail result = couponDetailService.create(couponDetail);
        Assert.assertNotNull(result);
        System.out.println(result);
    }
}