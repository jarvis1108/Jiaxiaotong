package com.dxtwangxiao.strongestsuperscholar.module.question.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;
import java.util.Date;

/**
 * Created by 孙伟浩 on 2018/9/8.
 */
@Entity
@Table(name = "t_QST_QuestionLib")
public class QuestionLib {

    @Id
    @Column(name = "QuestionLibID")
    private String questionLibID;

    @Column(name = "QuestionLibNo")
    private String questionLibNo;

    @Column(name = "QuestionLibCode")
    private String questionLibCode;

    @Column(name = "UserID")
    private String userID;

    @Column(name = "IsDefaultQuestionLib")
    private byte isDefaultQuestionLib;

    @Column(name = "IsPublic")
    private byte isPublic;

    @Column(name = "QuestionLibDes")
    private String questionLibDes;

    @Column(name = "QuestionLibTitle")
    private String questionLibTitle;

    @Column(name = "CreateTime")
    private Date createTime;

    @Column(name = "LastUpdTime")
    private Date lastUpdTime;

    @Column(name = "CreateUser")
    private String createUser;

    @Column(name = "LastUpdUser")
    private String lastUpdUser;

    @Column(name = "IsVaild")
    private byte isVaild;

    @Column(name = "Remark")
    private String remark;

    public QuestionLib(){}

    public QuestionLib(String questionLibID, String questionLibNo, String questionLibCode, String userID, byte isDefaultQuestionLib, byte isPublic, String questionLibDes, String questionLibTitle, Date createTime, Date lastUpdTime, String createUser, String lastUpdUser, byte isVaild, String remark) {
        this.questionLibID = questionLibID;
        this.questionLibNo = questionLibNo;
        this.questionLibCode = questionLibCode;
        this.userID = userID;
        this.isDefaultQuestionLib = isDefaultQuestionLib;
        this.isPublic = isPublic;
        this.questionLibDes = questionLibDes;
        this.questionLibTitle = questionLibTitle;
        this.createTime = createTime;
        this.lastUpdTime = lastUpdTime;
        this.createUser = createUser;
        this.lastUpdUser = lastUpdUser;
        this.isVaild = isVaild;
        this.remark = remark;
    }

    public String getQuestionLibID() {
        return questionLibID;
    }

    public void setQuestionLibID(String questionLibID) {
        this.questionLibID = questionLibID;
    }

    public String getQuestionLibNo() {
        return questionLibNo;
    }

    public void setQuestionLibNo(String questionLibNo) {
        this.questionLibNo = questionLibNo;
    }

    public String getQuestionLibCode() {
        return questionLibCode;
    }

    public void setQuestionLibCode(String questionLibCode) {
        this.questionLibCode = questionLibCode;
    }

    public String getUserID() {
        return userID;
    }

    public void setUserID(String userID) {
        this.userID = userID;
    }

    public byte getIsDefaultQuestionLib() {
        return isDefaultQuestionLib;
    }

    public void setIsDefaultQuestionLib(byte isDefaultQuestionLib) {
        this.isDefaultQuestionLib = isDefaultQuestionLib;
    }

    public byte getIsPublic() {
        return isPublic;
    }

    public void setIsPublic(byte isPublic) {
        this.isPublic = isPublic;
    }

    public String getQuestionLibDes() {
        return questionLibDes;
    }

    public void setQuestionLibDes(String questionLibDes) {
        this.questionLibDes = questionLibDes;
    }

    public String getQuestionLibTitle() {
        return questionLibTitle;
    }

    public void setQuestionLibTitle(String questionLibTitle) {
        this.questionLibTitle = questionLibTitle;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Date getLastUpdTime() {
        return lastUpdTime;
    }

    public void setLastUpdTime(Date lastUpdTime) {
        this.lastUpdTime = lastUpdTime;
    }

    public String getCreateUser() {
        return createUser;
    }

    public void setCreateUser(String createUser) {
        this.createUser = createUser;
    }

    public String getLastUpdUser() {
        return lastUpdUser;
    }

    public void setLastUpdUser(String lastUpdUser) {
        this.lastUpdUser = lastUpdUser;
    }

    public byte getIsVaild() {
        return isVaild;
    }

    public void setIsVaild(byte isVaild) {
        this.isVaild = isVaild;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }

    @Override
    public String toString() {
        return "QuestionLib{" +
                "questionLibID='" + questionLibID + '\'' +
                ", questionLibNo='" + questionLibNo + '\'' +
                ", questionLibCode='" + questionLibCode + '\'' +
                ", userID='" + userID + '\'' +
                ", isDefaultQuestionLib=" + isDefaultQuestionLib +
                ", isPublic=" + isPublic +
                ", questionLibDes='" + questionLibDes + '\'' +
                ", questionLibTitle='" + questionLibTitle + '\'' +
                ", createTime=" + createTime +
                ", lastUpdTime=" + lastUpdTime +
                ", createUser='" + createUser + '\'' +
                ", lastUpdUser='" + lastUpdUser + '\'' +
                ", isVaild=" + isVaild +
                ", remark='" + remark + '\'' +
                '}';
    }
}
