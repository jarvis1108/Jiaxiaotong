CREATE TABLE `ub_progress_study` (
  `Id` varchar(32) NOT NULL DEFAULT '',
  `user_id` varchar(32) NOT NULL DEFAULT '' COMMENT '用户id',
  `progress_id` varchar(32) NOT NULL DEFAULT '' COMMENT '进度id',
  `periods_id` varchar(32) NOT NULL DEFAULT '' COMMENT '课程期数id',
  `recite_status` tinyint(3) NOT NULL DEFAULT '0' COMMENT '背诵状态 0：未开始 1：背诵完成',
  `recite_score` int(11) NOT NULL DEFAULT '0' COMMENT '背诵得分',
  `recite_record` varchar(255) NOT NULL DEFAULT '' COMMENT '背诵录音地址',
  `read_status` tinyint(3) NOT NULL DEFAULT '0' COMMENT '阅读状态 0：未开始 1：未完成 2：已完成',
  `read_score` int(11) NOT NULL DEFAULT '0',
  `question_status` tinyint(3) NOT NULL DEFAULT '0' COMMENT '做题状态 0：未开始 1：已完成',
  `question_score` int(11) NOT NULL DEFAULT '0' COMMENT '成绩',
  `sign_status` tinyint(3) NOT NULL DEFAULT '0' COMMENT '打卡状态 0: 未打卡 1：已打卡 2：补打成功',
