#
# Structure for table "ub_order"
#

CREATE TABLE `ub_order` (
  `Id` varchar(32) NOT NULL DEFAULT '',
  `order_no` varchar(255) NOT NULL DEFAULT '' COMMENT '订单编号',
  `order_type` varchar(255) DEFAULT NULL COMMENT '订单类型：1 阅读订单',
  `order_status` tinyint(3) NOT NULL DEFAULT '0' COMMENT '订单状态 0：下单 1：已支付 2：支付成功 3：支付失败 4：订单超时 5：订单已取消',
  `user_id` varchar(32) NOT NULL DEFAULT '' COMMENT '客户id',
  `order_fee` decimal(10,2) unsigned NOT NULL DEFAULT '0.00' COMMENT '订单金额',
  `order_amount` decimal(10,2) unsigned DEFAULT '0.00' COMMENT '订单实付金额',
  `order_create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '订单创建时间',
  `order_modify_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '订单修改时间',
  `order_pay_time` datetime DEFAULT NULL COMMENT '订单付款时间',
  `order_pay_channel` tinyint(3) unsigned DEFAULT '0' COMMENT '订单支付渠道 0：微信支付 1：支付宝 2：内部支付',
  PRIMARY KEY (`Id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='订单表';

