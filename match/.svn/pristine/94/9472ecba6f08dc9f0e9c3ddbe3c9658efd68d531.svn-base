  `study_modify_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '学习修改时间',
  `is_deleted` tinyint(3) NOT NULL DEFAULT '0' COMMENT '逻辑删除 0：未删除 1：已删除',
  PRIMARY KEY (`Id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='用户进度跟踪';

