#
# Structure for table "uc_user"
#

CREATE TABLE `uc_user` (
  `Id` varchar(32) NOT NULL DEFAULT '' COMMENT '用户id',
  `user_name` varchar(255) DEFAULT '' COMMENT '用户名 当用户来源不为用户名密码注册时，此项可为空',
  `user_number` varchar(255) DEFAULT '' COMMENT '用户编号',
  `user_nickname` varchar(255) DEFAULT NULL COMMENT '用户昵称',
  `user_password` varchar(255) DEFAULT NULL COMMENT '用户登录密码 当用户手机号注册、微信来源，此项可为空',
