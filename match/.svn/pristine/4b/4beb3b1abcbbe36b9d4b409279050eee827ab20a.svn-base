<template name="index$71079ed0">
  <view class="_div data-v-41a26fea">
    <image id="bg" src="/static/images/bg@3x.png" class="_img data-v-41a26fea"></image>
    <image id="question_bg" src="/static/images/exerciseReview/subject-bg@3x.png" class="_img data-v-41a26fea"></image>
    <view id="content" bindtouchstart="handleProxy" bindtouchmove="handleProxy" bindtouchend="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_div data-v-41a26fea {{contentAni}}">
      <view id="bar" class="_div data-v-41a26fea">
        <image wx:if="{{questionIndex>1}}" bindtap="handleProxy" class="_img data-v-41a26fea arrow" id="leftArrow" src="/static/images/exerciseReview/leftArrow@3x.png" data-eventid="{{'0'}}" data-comkey="{{$k}}"></image>
        <view class="_div data-v-41a26fea questionId">
          <label class="_span data-v-41a26fea">第</label>
          <label id="questionIndex" class="_span data-v-41a26fea">{{questionIndex}}</label>
          <label class="_span data-v-41a26fea">题</label>
        </view>
        <image wx:if="{{questionIndex<questionList.length}}" bindtap="handleProxy" class="_img data-v-41a26fea arrow" id="rightArrow" src="/static/images/exerciseReview/rightArrow@3x.png" data-eventid="{{'1'}}" data-comkey="{{$k}}"></image>
      </view>
      <view id="questionContent" class="_div data-v-41a26fea">
        <label class="_span data-v-41a26fea"> {{questionList[questionIndex-1].questionContent}} </label>
      </view>
      <view id="hr" class="_hr data-v-41a26fea"></view>
      <view id="result" class="_div data-v-41a26fea {{resultColor}}">{{result}}</view>
      <view id="choiceList" class="_div data-v-41a26fea">
        <view class="_div data-v-41a26fea choice" id="{{'choice'+choice.label}}" wx:key="choice" key="{{choice}}" wx:for="{{questionList[questionIndex-1].choiceList}}" wx:for-index="index" wx:for-item="choice">
          <image class="_img data-v-41a26fea choice_img" src="{{choice.img}}"></image>
          <label id="{{choice.style}}" class="_span data-v-41a26fea choice_content">{{choice.label}}.{{choice.content}}</label>
          <image class="_img data-v-41a26fea errorIcon" wx:if="{{choice.isRight === -1}}" src="/static/images/exerciseReview/Errorlabel@3x.png"></image>
          <image class="_img data-v-41a26fea correctIcon" wx:if="{{choice.isRight === 1}}" src="/static/images/exerciseReview/Correctlabel@3x.png"></image>
        </view>
      </view>
    </view>
    <view id="iconList" class="_div data-v-41a26fea">
      <view id="collect" bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}" class="_div data-v-41a26fea">
        <image class="_img data-v-41a26fea icon" id="collectIcon" src="/static/images/exerciseReview/Collection-icon@3x.png"></image>
        <label class="_span data-v-41a26fea text_12">收藏</label>
      </view>
      <view id="correct" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}" class="_div data-v-41a26fea">
        <image class="_img data-v-41a26fea icon" id="correctIcon" src="/static/images/exerciseReview/Errorcorrection-icon@3x.png"></image>
        <label class="_span data-v-41a26fea text_12">纠错</label>
      </view>
      <view class="_div data-v-41a26fea">
        <button id="share" open-type="share" class="_button data-v-41a26fea">
          <view class="_div data-v-41a26fea">
            <image class="_img data-v-41a26fea icon" id="shareIcon" src="/static/images/exerciseReview/Share-icon@3x.png"></image>
            <label class="_span data-v-41a26fea text_12" style="top: 50%">分享</label>
          </view>
        </button>
      </view>
    </view>
    <image id="halo" src="/static/images/exerciseReview/Halo@3x.png" class="_img data-v-41a26fea"></image>
    <view id="details" class="_div data-v-41a26fea">
      <view id="analysis" class="_div data-v-41a26fea">
        <label class="_span data-v-41a26fea">解析:{{questionList[questionIndex-1].analysis}}</label>
      </view>
      <view id="source" class="_div data-v-41a26fea">
        <label class="_span data-v-41a26fea">出处：{{questionList[questionIndex-1].source}}</label>
      </view>
      <view id="knowledge" class="_div data-v-41a26fea">
        <label class="_span data-v-41a26fea">考点:{{questionList[questionIndex-1].knowledge}}</label>
      </view>
    </view>
  </view>
</template>