import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.test.context.SpringBootTest;
import org.springframework.test.context.junit4.SpringRunner;

import java.util.List;

import static org.junit.Assert.*;
@SpringBootTest
@RunWith(SpringRunner.class)
public class GradeInfoRepositoryTest {

    @Autowired
    private GradeInfoRepository gradeInfoRepository;
    @Test
    public void findAll() {
        List<GradeInfo> gradeInfos=gradeInfoRepository.findAll();
        assertNotEquals(0,gradeInfos.size());
        gradeInfos.forEach(System.out::println);

