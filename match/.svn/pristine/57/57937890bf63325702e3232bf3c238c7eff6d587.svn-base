package com.dxtwangxiao.strongestsuperscholar.config;

public class RedisCfg
{
    /**
     * Session的主机
     */
    private String strRedisHost;

    /**
     * Redis的端口号
     */
    private  String strRedisPort;

    /**
     * Redis的用户名
     */
    private  String strRedisUser;

    /**
     * Redis的密码
     */
    private  String strRedisPsw;
    /**
     * Redis的分布方式
     */
    private  String dataDistributionWay;

    /**
     * 实现功能:缺省无参构造函数
     */
    public RedisCfg()
    {

    }

    public String getStrRedisHost()
    {
        return strRedisHost;
    }

    public void setStrRedisHost(String strRedisHost)
    {
        this.strRedisHost = strRedisHost;
    }

    public String getStrRedisPort()
    {
        return strRedisPort;
    }

    public void setStrRedisPort(String strRedisPort)
    {
        this.strRedisPort = strRedisPort;
    }

    public String getStrRedisUser()
    {
        return strRedisUser;
    }

    public void setStrRedisUser(String strRedisUser)
    {
        this.strRedisUser = strRedisUser;
    }

    public String getStrRedisPsw()
    {
        return strRedisPsw;
    }

    public void setStrRedisPsw(String strRedisPsw)
    {
        this.strRedisPsw = strRedisPsw;
    }



    public String getDataDistributionWay() {
        return dataDistributionWay;
    }

    public void setDataDistributionWay(String dataDistributionWay) {
        this.dataDistributionWay = dataDistributionWay;
    }

}
