<template name="index$9b22684a">
  <view class="_div data-v-3577e9c6 st-exchange-record-container">
    <view wx:if="{{recordList.length === 0}}" class="_div data-v-3577e9c6 st-exchange-record-noRecord-span">暂时还没有兑换记录</view>
    <view class="_ul data-v-3577e9c6 st-exchange-record-ul">
      <view class="_li data-v-3577e9c6 st-exchange-record-li" wx:key="index" key="{{index}}" wx:for="{{recordList}}" wx:for-index="index" wx:for-item="item">
        <image class="_img data-v-3577e9c6 st-exchange-record-li-bg" wx:if="{{(index%3) === 0}}" src="/static/images/topupRecord/Blue_bg.png"></image>
        <image class="_img data-v-3577e9c6 st-exchange-record-li-bg" wx:elif="{{(index%3) === 1}}" src="/static/images/topupRecord/Green_bg.png"></image>
        <image class="_img data-v-3577e9c6 st-exchange-record-li-bg" wx:else src="/static/images/topupRecord/Cyan_bg.png"></image>
        <image class="_img data-v-3577e9c6 st-exchange-record-li-icon" src="{{item.productImage}}"></image>
        <view class="_div data-v-3577e9c6 st-exchange-record-li-name-div">
          <label wx:if="{{(index%3) === 0}}" class="_span data-v-3577e9c6 st-exchange-record-li-blue-name">{{item.productName}}</label>
          <label wx:elif="{{(index%3) === 1}}" class="_span data-v-3577e9c6 st-exchange-record-li-green-name">{{item.productName}}</label>
          <label wx:else class="_span data-v-3577e9c6 st-exchange-record-li-cyan-name">{{item.productName}}</label>
        </view>
        <label class="_span data-v-3577e9c6 st-exchange-record-li-price">{{item.productPrice}}</label>
        <image class="_img data-v-3577e9c6 st-exchange-record-li-coin-img" src="/static/images/exchangeRecord/Goldcoin_icon.png"></image>
        <view class="_div data-v-3577e9c6 st-exchange-record-li-datatime">
          <label wx:if="{{(index%3) === 0}}" class="_span data-v-3577e9c6 st-exchange-record-li-blue-datatime">{{item.createTime}}</label>
          <label wx:elif="{{(index%3) === 1}}" class="_span data-v-3577e9c6 st-exchange-record-li-green-datatime">{{item.createTime}}</label>
          <label wx:else class="_span data-v-3577e9c6 st-exchange-record-li-cyan-datatime">{{item.createTime}}</label>
        </view>
      </view>
    </view>
  </view>
</template>