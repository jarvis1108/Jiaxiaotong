<template name="questioncard$39029e89">
  <view class="_div data-v-5c28c335 bs-container-questioncard-div">
    <image class="_img data-v-5c28c335 bs-bg-img {{backAnimation}}" src="/copy-asset/static/images/battle/subject-bg@3x.png"></image>
    <image class="_img data-v-5c28c335 bs-divider-img {{backAnimation}}" src="/copy-asset/static/images/battle/line@3x.png"></image>
    <view class="_div data-v-5c28c335 bs-Question-div  {{animation}}" wx:if="{{questionType && showQuestion}}">
      <view class="_div data-v-5c28c335 bs-text2 bs-chooseTitle-div"> {{ question.title }}{{ question.type }} </view>
      <view class="_div data-v-5c28c335 bs-content-div">
        <view class="_div data-v-5c28c335 bs-option-div" bindtap="handleProxy" wx:key="item" key="{{item}}" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}" wx:for="{{labelList}}" wx:for-index="index" wx:for-item="item">
          <image class="_img data-v-5c28c335 bs-bg-img" src="/copy-asset/static/images/battle/Optionbox@3x.png"></image>
          <image class="_img data-v-5c28c335 bs-bg-img" src="/copy-asset/static/images/battle/Erroroption-bg@3x.png" v-show="{{'isChoose'+item}}"></image>
          <image id="bs-error-icon" src="/copy-asset/static/images/battle/Errorlabel@3x.png" v-show="{{'isChoose'+item}}" class="_img data-v-5c28c335"></image>
          <image class="_img data-v-5c28c335 bs-bg-img" src="/copy-asset/static/images/battle/Checktheoptionbox@3x.png" v-show="{{'isTrueA'+item}}"></image>
          <image id="bs-true-icon" src="/copy-asset/static/images/battle/Correctlabel@3x.png" v-show="{{'isTrueA'+item}}" class="_img data-v-5c28c335"></image>
          <label class="_span data-v-5c28c335"></label>
        </view>
      </view>
    </view>
    <view class="_div data-v-5c28c335 bs-Question-div  {{animation}}" wx:if="{{!questionType && showQuestion}}">
      <view class="_div data-v-5c28c335 bs-text2 bs-recordTitle-div"> {{ question.title }}{{ question.type }} </view>
      <view class="_div data-v-5c28c335 bs-text2 bs-content-div"> {{ question.content }} </view>
      <image class="_img data-v-5c28c335 bs-recordButton-img" bindtap="handleProxy" hidden="{{!(!recording)}}" src="/copy-asset/static/images/battle/Startrecording@3x.png" data-eventid="{{'1'}}" data-comkey="{{$k}}"></image>
      <image class="_img data-v-5c28c335 bs-recordButton-img" bindtap="handleProxy" hidden="{{!(recording)}}" src="/copy-asset/static/images/battle/suspend@3x.png" data-eventid="{{'2'}}" data-comkey="{{$k}}"></image>
    </view>
  </view>
</template>