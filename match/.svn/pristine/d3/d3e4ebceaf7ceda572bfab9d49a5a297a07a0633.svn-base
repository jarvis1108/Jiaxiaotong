package com.dxtwangxiao.strongestsuperscholar.web.dto;

import com.dxtwangxiao.strongestsuperscholar.module.base.entity.Subject;

import java.io.Serializable;

/**
 * Created By Devin
 * 2018-09-03 下午 07:51
 */
public class SubjectDTO implements Serializable {

    private Subject subject;

    public SubjectDTO(Subject subject) {
        this.subject = subject;
    }

    public String getSubjectId() {
        return subject.getSubjectId();
    }

    public String getSubjectNo() {
        return subject.getSubjectNo();
    }

    public String getSubjectName() {
        return subject.getSubjectName();
    }

    public String getSubjectShortName() {
        return subject.getSubjectShortName();
    }

    public String getShowText() {
        return subject.getShowText();
    }
}
