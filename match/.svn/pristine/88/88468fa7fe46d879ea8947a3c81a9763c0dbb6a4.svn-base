package com.dxtwangxiao.strongestsuperscholar.web.util.commconstant;

/**
 * 常量设置，主要针对错误码和错误信息（公共的）
 * <p>
 * Created by Devin
 * 2018-07-02 上午 09:08
 */
public enum CommConst {

    ERR_SUCCESS("000", "请求成功"),
    ERR_REQ_TYPE_NULL("001", "请求类型不能为空"),
    ERR_REQ_PARAM("002", "请求参数异常");

    private String code;

    private String message;

    CommConst(String code, String message) {
        this.code = code;
        this.message = message;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }

}
