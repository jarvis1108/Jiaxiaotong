@import "/static/css/vendor.wxss";

.st-my-camp-container {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgb(207, 235, 255);
}
.st-my-camp-header-img {
  position: absolute;
  top: 2%;
  height: 20%;
  left: 5%;
  width: 90%;
}
.st-my-camp-mine-div {
  position: absolute;
  top: 23%;
  left: 5%;
  width: 90%;
  height: 15%;
}
.st-my-camp-memberList-div {
  position: absolute;
  top: 38.5%;
  left: 5%;
  width: 90%;
  height: 40%;
  overflow: auto;
}
.st-my-camp-memberList-div ._ul {
  position: absolute;
  width: 100%;
  height: 100%;
  animation: slideFromTop 1.5s;
}
.st-my-camp-memberList-div ._ul ._li {
  position: relative;
  height: 32%;
  width: 100%;
  margin-bottom: 2%;
}
.st-my-camp-invite-btn {
  position: absolute;
  width: 26%;
  left: 37%;
  height: 5.5%;
  z-index: 10;
  overflow: auto;
  animation: slideFromBottom 2.5s;
}
.st-my-camp-board-bg {
  position: absolute;
  width: 100%;
  height: 100%;
}
.st-my-camp-usericon {
  position: absolute;
  width: 104rpx;
  height: 104rpx;
  border-radius: 50%;
  border: 6rpx solid #fff;
  background: rgb(255, 243, 102);
  left: 5%;
  top: 15%;
}
.st-my-camp-username {
  position: absolute;
  left: 25%;
  top: 20%;
  font-size: 26rpx;
  font-weight: bold;
  color: #fff;
}
.st-my-camp-usertitle {
  position: absolute;
  right: 10%;
  top: 20%;
  font-size: 26rpx;
  font-weight: bold;
  color: #fff;
}
.st-my-camp-hr {
  position: absolute;
  left: 22.5%;
  height: 4rpx;
  background: #fff;
  top: 45%;
  width: 75%;
}
.st-my-camp-userlevel {
  position: absolute;
  left: 25%;
  top: 50%;
  font-size: 20rpx;
  font-weight: bold;
  color: #fff;
}
.st-my-camp-memberNum {
  position: absolute;
  left: 55%;
  top: 50%;
  font-size: 20rpx;
  font-weight: bold;
  color: rgb(179, 255, 240);
}
@keyframes slideFromTop {
from {
    transform: translateY(-100%);
}
}
@keyframes slideFromBottom {
0%, 60% {
    transform: translateY(100%);
    opacity: 0;
}
100% {
    transform: translateX(0);
    opacity: 1;
}
}