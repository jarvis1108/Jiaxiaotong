<template name="group-battle-waiting$70673e82">
  <view class="_div data-v-72f0ac1c st-background-base-div">
    <image class="_img data-v-72f0ac1c st-background-bg-div" src="/static/images/bg.png"></image>
    <view id="st-status-text-div" hidden="{{!(isCreator)}}" class="_div data-v-72f0ac1c">等待成员加入...</view>
    <view id="st-status-text-div" hidden="{{!(!isCreator)}}" class="_div data-v-72f0ac1c">等待房主开局...</view>
    <view class="_div data-v-72f0ac1c st-players-div">
      <view class="_div data-v-72f0ac1c st-players-flex-div" bindtouchstart="handleProxy" bindtouchmove="handleProxy" bindtouchend="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}">
        <view wx:if="{{selectPage === 1}}" class="_div data-v-72f0ac1c st-player-image-box" wx:key="index" key="{{index}}" wx:for="{{firstTenPlayerList}}" wx:for-index="index" wx:for-item="player">
          <image wx:if="{{creator.userId === player.userId}}" src="/static/images/groupBattleWaiting/Owner_icon.png" class="_img data-v-72f0ac1c st-owner-icon"></image>
          <image wx:if="{{playerList.length <= 3}}" class="_img data-v-72f0ac1c st-bigger-headImage-bg" src="{{player.headImage}}"></image>
          <image wx:else class="_img data-v-72f0ac1c st-headImage-bg" src="{{player.headImage}}"></image>
        </view>
        <view wx:if="{{selectPage === 2}}" class="_div data-v-72f0ac1c st-player-image-box" wx:key="index" key="{{index}}" wx:for="{{lastTenPlayerList}}" wx:for-index="index" wx:for-item="player">
          <image class="_img data-v-72f0ac1c st-headImage-bg" src="{{player.headImage}}"></image>
        </view>
      </view>
      <view wx:if="{{playerList.length > 10}}" class="_div data-v-72f0ac1c st-page-point-div">
        <view wx:if="{{selectPage === 1}}" class="_div data-v-72f0ac1c st-page-select-point"></view>
        <view wx:if="{{selectPage === 2}}" class="_div data-v-72f0ac1c st-page-point"></view>
        <view wx:if="{{selectPage === 1}}" class="_div data-v-72f0ac1c st-page-point"></view>
        <view wx:if="{{selectPage === 2}}" class="_div data-v-72f0ac1c st-page-select-point"></view>
      </view>
    </view>
    <view id="st-game-start-div" hidden="{{!(isCreator)}}" wx:if="{{playerNumber > 1}}" class="_div data-v-72f0ac1c">
      <image bindtap="handleProxy" src="/static/images/groupBattleWaiting/Startthecompetition_btn.png" class="_img data-v-72f0ac1c st-background-bg-div" data-eventid="{{'1'}}" data-comkey="{{$k}}"></image>
    </view>
    <view id="icons" class="_div data-v-72f0ac1c">
      <view id="giveup-div" class="_div data-v-72f0ac1c">
        <image bindtap="handleProxy" class="_img data-v-72f0ac1c giveUp" src="/static/images/groupBattleWaiting/Giveup_icon.png" data-eventid="{{'2'}}" data-comkey="{{$k}}"></image>
        <label id="giveup-text" class="_span data-v-72f0ac1c">放弃</label>
      </view>
      <view id="invite-div" class="_div data-v-72f0ac1c">
        <button open-type="{{isShareable}}" class="_button data-v-72f0ac1c invite">
          <image src="/static/images/groupBattleWaiting/Invitation_icon.png" class="_img data-v-72f0ac1c invite_img"></image>
        </button>
        <label id="invite-text" class="_span data-v-72f0ac1c">邀请</label>
      </view>
    </view>
  </view>
</template>