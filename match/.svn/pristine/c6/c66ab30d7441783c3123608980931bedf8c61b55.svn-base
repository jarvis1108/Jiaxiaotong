rc=\"/static/images/index/Goldcoin@2x.png\"/>\n      <p>\n        <span id=\"user_coin\" class=\"text_28\">{{userInfo.coin}}</span>\n      </p>\n\n      <p>\n        <span id=\"grade\" style=\"left: 10%\">{{userInfo.externalLevel}}</span>\n        <span class=\"text_24\" style=\"left: 12%\">级</span>\n      </p>\n\n      <div class=\"st-personal-center-level-outer\">\n        <div :style=\"'width:'+progress+'%'\" class=\"st-personal-center-level-inner\"></div>\n      </div>\n\n      <p>\n        <span class=\"text_28\" style=\"right: 38%\">绩点</span>\n        <span id=\"gpa\" class=\"text_28\" style=\"right: 30%\">{{userInfo.gradePoint}}</span>\n      </p>\n      <p>\n        <span class=\"text_28\" style=\"right: 20%\">学点</span>\n        <span id=\"lp\" class=\"text_28\" style=\"right: 12%\">{{userInfo.learningPoint}}</span>\n      </p>\n    </div>\n\n    <!-- function block -->\n    <div>\n      <img id=\"dualmeet\" @click=\"to_dualmeet\" src=\"/static/images/index/dualmeet-bg@2x.png\"/>\n      <p>\n        <span class=\"text_34\" style=\"left: 9%;top: 50%\">对抗赛</span>\n      </p>\n\n      <img id=\"exercise\" @click=\"to_exercise\" src=\"/static/images/index/paradeground-bg@2x.png\"/>\n      <p>\n        <span class=\"text_34\" style=\"left: 9%;top: 74%\">知识练兵场</span>\n      </p>\n\n      <img id=\"goldMall\" @click=\"to_goldMall\" src=\"/static/images/index/Gold_icon@2x.png\"/>\n      <p>\n        <span class=\"text_34\" style=\"left: 9%;top: 90%\">金币商城</span>\n      </p>\n\n      <button @click=\"change_pk\" open-type=\"share\" class=\"pk_btn\">\n        <img id=\"pk\" @click=\"to_pk\" src=\"/static/images/index/pk-bg@2x.png\"/>\n      </button>\n      <p>\n        <span class=\"text_34\" style=\"left: 55%;top: 42%\">好友对战</span>\n      </p>\n\n      <img id=\"rank\" @click=\"to_rank\" src=\"/static/images/index/Rankinglist-bg@2x.png\"/>\n      <p>\n        <span class=\"text_34\" style=\"left: 55%;top: 58%\">排行榜</span>\n      </p>\n\n      <img id=\"topup\" @click=\"to_topup\" src=\"/static/images/index/top-ups-bg@2x.png\"/>\n      <p>\n        <span class=\"text_34\" style=\"left: 55%;top: 74%\">充值</span>\n      </p>\n\n      <button @click=\"change_group\" open-type=\"share\" class=\"battle_btn\">\n         <img id=\"groupBattle\" @click=\"to_groupBattle\" src=\"/static/images/index/Groupgame_icon@2x.png\"/>\n      </button>   \n      <p>\n        <span class=\"text_34\" style=\"left: 55%;top: 90%\">群比赛</span>\n      </p>\n\n    </div>\n\n    <gradeDialog v-if=\"showGradeDialog\" @listenToChildEvent=\"getGradeInfoFromChild\"/>\n\n  </div>\n</template>\n\n<script>\nimport wxs from \"../../utils/wx\";\nimport gradeDialog from \"../../components/grade-dialog\";\nimport { socketUrl } from \"../../config\";\nexport default {\n  components: { gradeDialog },\n  name: \"index.vue\",\n  data() {\n    return {\n      userInfo: {},\n      showGradeDialog: false, //是否显示年级选择弹窗\n      socket: null,\n      isGroup: false //是否是群内pk\n    };\n  },\n  async onLoad() {\n    try {\n      if (await wxs.getStorage(\"notFirstLogin\")) {\n        console.log(\"不是首次登陆\");\n        // 非第一次登陆，直接获取用户信息\n        await this.$store.dispatch(\"login\");\n        this.userInfo = this.$store.state.userInfo;\n      }\n    } catch (e) {\n      //首次登陆选择年级信息\n      wxs.setStorage(\"notFirstLogin\", true);\n      console.log(\"首次登陆\");\n      this.showGradeDialog = true;\n    }\n    wxs.showShareMenu(true);\n  },\n  onShareAppMessage(res) {\n    let matchId = \"001\";\n    let Path = \"pages/friend-pk/main?matchId=001&isCreator=false\";\n    if (this.isGroup) {\n      Path = \"pages/group-battle-waiting/main?matchId=001&isCreator=false\";\n    }\n    console.log(\"matchId:\" + matchId);\n    if (res.from === \"button\") {\n    }\n    return {\n      title: this.userInfo.wechatName + \"向你发出了智商pk，点击应战！\",\n      path: Path,\n      success: res => {\n        console.log(\"分享成功，本次对战编码为\" + matchId);\n        if (this.isGroup) {\n          wxs.navigateTo(\n         