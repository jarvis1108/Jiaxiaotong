package entity;

import util.TreeNode;

/**
 * @author miaoyu
 * @date 2018/7/3 10:12
 */
public class Subject {
    private String InitID;
    private String InitNo;
    private Byte isCorrect;
    private String SubjectID;
    private String SubjectNo;
    private String SubjectName;
    private Character SubjectType;
    private String SubejctShortName;
    private String SubjectGradeType;
    private String SubjectGrade;
    private String CreateTime;
    private String CreateUser;
    private String LastUpdTime;
    private String LastUpdUser;
    private Byte IsValid;
    private String Remark;


    public Subject(TreeNode treeNode){
        this.CreateTime = treeNode.getCreateTime();
        this.CreateUser = treeNode.getCreateUser();
        this.SubjectNo = treeNode.getLevel();
        this.SubjectName = treeNode.getName();
        this.Remark = treeNode.getDescription();
        this.LastUpdTime = treeNode.getLastUpdateTime();
        this.LastUpdUser = treeNode.getLastUpdateUser();
    }

    public String getSubjectNo() {
        return SubjectNo;
    }

    public void setSubjectNo(String subjectNo) {
        SubjectNo = subjectNo;
    }

    public String getSubjectName() {
        return SubjectName;
    }

    public void setSubjectName(String subjectName) {
        SubjectName = subjectName;
    }

    public String getRemark() {
        return Remark;
    }

    public void setRemark(String remark) {
        Remark = remark;
    }

    public String getCreateTime() {
        return CreateTime;
    }

    public void setCreateTime(String createTime) {
        CreateTime = createTime;
    }

    public String getCreateUser() {
        return CreateUser;
    }

    public void setCreateUser(String createUser) {
        CreateUser = createUser;
    }

    public String getLastUpdTime() {
        return LastUpdTime;
    }

    public void setLastUpdTime(String lastUpdTime) {
        LastUpdTime = lastUpdTime;
    }

    public String getLastUpdUser() {
        return LastUpdUser;
    }

    public void setLastUpdUser(String lastUpdUser) {
        LastUpdUser = lastUpdUser;
    }

    public String getSubjectID() {
        return SubjectID;
    }

    public void setSubjectID(String subjectID) {
        SubjectID = subjectID;
    }

    public Character getSubjectType() {
        return SubjectType;
    }

    public void setSubjectType(Character subjectType) {
        SubjectType = subjectType;
    }

    public String getSubjectGrade() {
        return SubjectGrade;
    }

    public void setSubjectGrade(String subjectGrade) {
        SubjectGrade = subjectGrade;
    }

    public String getSubjectGradeType() {
        return SubjectGradeType;
    }

    public void setSubjectGradeType(String subjectGradeType) {
        SubjectGradeType = subjectGradeType;
    }

    public Byte getIsValid() {
        return IsValid;
    }

    public void setIsValid(Byte isValid) {
        IsValid = isValid;
    }

    public String getSubejctShortName() {
        return SubejctShortName;
    }

    public void setSubejctShortName(String subejctShortName) {
        SubejctShortName = subejctShortName;
    }

    public String getInitID() {
        return InitID;
    }

    public void setInitID(String initID) {
        InitID = initID;
    }

    public String getInitNo() {
        return InitNo;
    }

    public void setInitNo(String initNo) {
        InitNo = initNo;
    }

    public Byte getIsCorrect() {
        return isCorrect;
    }

    public void setIsCorrect(Byte isCorrect) {
        this.isCorrect = isCorrect;
    }
}
