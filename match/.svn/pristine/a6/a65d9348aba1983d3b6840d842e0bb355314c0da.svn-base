    <view id="content" bindtouchstart="handleProxy" bindtouchmove="handleProxy" bindtouchend="handleProxy" data-eventid="{{'6'}}" data-comkey="{{$k}}" class="_div data-v-177164f8 {{contentAni}}">
      <view id="bar" class="_div data-v-177164f8">
