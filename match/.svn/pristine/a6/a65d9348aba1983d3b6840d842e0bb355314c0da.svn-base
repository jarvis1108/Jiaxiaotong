          <label class="_span data-v-177164f8">题</label>
        </view>
        <image wx:if="{{questionIndex<questionList.length}}" bindtap="handleProxy" class="_img data-v-177164f8 arrow" id="rightArrow" src="/static/images/exerciseReview/rightArrow.png" data-eventid="{{'1'}}" data-comkey="{{$k}}"></image>
      </view>
      <view id="questionContent" class="_div data-v-177164f8">
        <label class="_span data-v-177164f8"> {{questionList[questionIndex-1].question}} </label>
