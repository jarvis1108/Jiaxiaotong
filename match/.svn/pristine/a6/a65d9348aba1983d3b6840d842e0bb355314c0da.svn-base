          <image class="_img data-v-177164f8 errorIcon" wx:if="{{indexList[questionIndex-1].choiceList[0].isRight === -1}}" src="/static/images/exercise/Errorlabel.png"></image>
          <image class="_img data-v-177164f8 correctIcon" wx:if="{{indexList[questionIndex-1].choiceList[0].isRight === 1}}" src="/static/images/exercise/Correctlabel.png"></image>
        </view>
        <view class="_div data-v-177164f8 choice" bindtap="handleProxy" id="{{choiceA}}" key="{{choice}}" data-eventid="{{'3'}}" data-comkey="{{$k}}">
