<template name="index$16da6106">
  <view class="_div data-v-5ef2af20">
    <image id="bg" src="/static/images/bg@3x.png" class="_img data-v-5ef2af20"></image>
    <image class="_img data-v-5ef2af20 winImg" wx:if="{{win}}" src="/static/images/battleResult/Challengesuccess@3x.png"></image>
    <image class="_img data-v-5ef2af20 winImg" wx:if="{{!win}}" src="/static/images/battleResult/Challengefailure@3x.png"></image>
    <view class="_div data-v-5ef2af20 info">
      <view id="user" class="_div data-v-5ef2af20" style=" {{'width:' + userWidth + '%'}}">
        <image id="userIcon" src="{{user.icon}}" class="_img data-v-5ef2af20"></image>
        <label id="userName" class="_span data-v-5ef2af20">{{user.name}}</label>
        <label id="userAnswerNum" style="font-weight: bold;" class="_span data-v-5ef2af20">答对{{userAnswerNum}}题</label>
        <view class="_div data-v-5ef2af20 mark" id="userMark">
          <label class="_span data-v-5ef2af20">{{animatedUserMark}}</label>
          <label style="font-size: 16px" class="_span data-v-5ef2af20">分</label>
          <view class="_div data-v-5ef2af20">
            <label class="_span data-v-5ef2af20 flag">—</label>
          </view>
        </view>
      </view>
      <view id="opponent" class="_div data-v-5ef2af20" style=" {{'width:' + opponentWidth + '%'}}">
        <image id="opponentIcon" src="{{opponent.icon}}" class="_img data-v-5ef2af20"></image>
        <label id="opponentName" class="_span data-v-5ef2af20">{{opponent.name}}</label>
        <label id="opponentAnswerNum" style="font-weight: bold;" class="_span data-v-5ef2af20">答对{{opponentAnswerNum}}题</label>
        <view class="_div data-v-5ef2af20 mark" id="opponentMark">
          <label class="_span data-v-5ef2af20">{{animatedOpponentMark}}</label>
          <label style="font-size: 18px" class="_span data-v-5ef2af20">分</label>
          <view class="_div data-v-5ef2af20">
            <label class="_span data-v-5ef2af20 flag">—</label>
          </view>
        </view>
      </view>
    </view>
    <view class="_div data-v-5ef2af20 table">
      <view style="display: table-row" class="_div data-v-5ef2af20">
        <view style="display: table-cell;width: 50%" class="_div data-v-5ef2af20">金币：</view>
        <view style="display: table-cell;width: 50%" class="_div data-v-5ef2af20">+{{coin}}</view>
      </view>
      <view style="display: table-row" class="_div data-v-5ef2af20">
        <view style="display: table-cell;width: 50%" class="_div data-v-5ef2af20">绩点：</view>
        <view style="display: table-cell;width: 50%" class="_div data-v-5ef2af20">+{{gpa}}</view>
      </view>
      <view style="display: table-row" class="_div data-v-5ef2af20">
        <view style="display: table-cell;width: 50%" class="_div data-v-5ef2af20">学点：</view>
        <view style="display: table-cell;width: 50%" class="_div data-v-5ef2af20">-{{lp}}</view>
      </view>
      <view style="display: table-row" class="_div data-v-5ef2af20">
        <view style="display: table-cell;width: 50%" class="_div data-v-5ef2af20">经验：</view>
        <view style="display: table-cell;width: 50%" class="_div data-v-5ef2af20">+{{experience}}</view>
      </view>
    </view>
    <image bindtap="handleProxy" class="_img data-v-5ef2af20 btn" id="continue" src="/static/images/battleResult/1.1Continue-btn@3x.png" data-eventid="{{'0'}}" data-comkey="{{$k}}"></image>
    <button wx:if="{{win}}" class="_button data-v-5ef2af20 btn" id="showoff" open-type="share">
      <image class="_img data-v-5ef2af20 btn_img" src="/static/images/battleResult/1.1Showoff-btn@3x.png"></image>
    </button>
    <button wx:if="{{!win}}" class="_button data-v-5ef2af20 btn" id="share" open-type="share">
      <image class="_img data-v-5ef2af20 btn_img" src="/static/images/battleResult/Shareresults-btn@3x.png"></image>
    </button>
    <label id="tip" class="_span data-v-5ef2af20">分享到微信群可获得50金币奖励</label>
  </view>
</template>