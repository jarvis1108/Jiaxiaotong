<template name="group-battle-result$22f81a99">
  <view class="_div data-v-56f52f08 st-background-base-div">
    <image class="_img data-v-56f52f08 st-background-bg-div" src="/static/images/bg.png"></image>
    <view class="_div data-v-56f52f08 st-result-detail-div">
      <image class="_img data-v-56f52f08 st-background-bg-div" src="/static/images/groupBattleResult/Performancedisplay_bg.png"></image>
      <view id="st-total-grade-p" class="_p data-v-56f52f08">{{animatedUserMark}}分</view>
      <view id="st-total-correct-p" class="_p data-v-56f52f08">答对{{userAnswerNum}}题</view>
      <view id="st-coin-change-p" class="_p data-v-56f52f08 st-detail-text-p">金币：    +{{isUserWin?100:0}}</view>
      <view id="st-gradepoint-change-p" class="_p data-v-56f52f08 st-detail-text-p">绩点：    +{{userCorrectNum}}</view>
      <view id="st-studypoint-change-p" class="_p data-v-56f52f08 st-detail-text-p">学点：    -{{userLearningPoit}}</view>
      <view id="st-expression-change-p" class="_p data-v-56f52f08 st-detail-text-p">经验：    +{{isUserWin?10:5}}</view>
    </view>
    <view class="_div data-v-56f52f08 st-player-range-div">
      <view class="_div data-v-56f52f08 st-player-detail-div" wx:key="player" key="{{player}}" wx:for="{{orderedPlayer}}" wx:for-index="player" wx:for-item="index">
        <view class="_div data-v-56f52f08 st-player-img-div">
          <image class="_img data-v-56f52f08 st-background-bg-div user-icon" src="{{player[2]}}"></image>
        </view>
        <view class="_p data-v-56f52f08">第{{index+1}}名</view>
        <view class="_p data-v-56f52f08">{{player[1]}}分</view>
        <view class="_div data-v-56f52f08 st-player-cup-div">"index === 0" class="st-background-bg-div" src="/static/images/groupBattleResult/Goldtrophy_icon.png" /> "index === 1" class="st-background-bg-div" src="/static/images/groupBattleResult/Silvertrophy_icon.png" /> "index === 2" class="st-background-bg-div"
          src="/static/images/groupBattleResult/Bronzetrophy_icon.png" /> </view>
      </view>
    </view>
    <view class="_div data-v-56f52f08 st-continue-game-div">
      <image bindtap="handleProxy" class="_img data-v-56f52f08 st-background-bg-div" src="/static/images/groupBattleResult/1.1Continue-btn.png" data-eventid="{{'0'}}" data-comkey="{{$k}}"></image>
    </view>
    <button class="_button data-v-56f52f08 st-share-result-div" id="showoff" open-type="share">
      <image class="_img data-v-56f52f08 btn_img" src="/static/images/groupBattleResult/1.1Showoff-btn.png"></image>
    </button>
  </view>
</template>